{
  "name": "free-enterprise-tracker",
  "version": "1.0.0",
  "description": "A project originally created with elm-spa",
  "scripts": {
    "start": "npm install && npm run build:dev && npm run dev",
    "build": "elm make src/Main.elm --optimize --output=public/dist/elm.compiled.js",
    "build:dev": "elm make src/Main.elm --debug --output=public/dist/elm.compiled.js || true",
    "dev": "elm-live src/Main.elm -u -d public -- --debug --output=public/dist/elm.compiled.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "elm": "0.19.1-3",
    "elm-live": "https://github.com/wking-io/elm-live#e317b4914c471addea7243c47f28dcebe27a5d36"
  }
}
